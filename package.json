{
  "name": "modern-english-learning-platform",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "description": "AI-powered English learning platform with smart dictionary, audio sync, and word frequency analysis",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "keywords": [
    "english-learning",
    "language-learning",
    "vue3",
    "typescript",
    "pwa",
    "education"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "postbuild": "node scripts/post-build.js",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:github": "npm run build && gh-pages -d dist -b gh-pages",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:unit": "vitest run",
    "clean": "rimraf dist node_modules/.vite"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "@vueuse/core": "^10.5.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "gh-pages": "^6.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kendall-hawk/learner-platform.git"
  },
  "homepage": "https://kendall-hawk.github.io/learner-platform/",
  "bugs": {
    "url": "https://github.com/YOUR-USERNAME/YOUR-REPO-NAME/issues"
  }
}